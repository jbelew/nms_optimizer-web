{
	"name": "nms_optimizer",
	"private": true,
	"version": "6.5.18",
	"type": "module",
	"scripts": {
		"dev": "vite",
		"prebuild": "node scripts/generate-radix-colors.mjs",
		"build": "tsc -b && vite build --mode production && node scripts/generate-ssg.mjs",
		"build:docker": "DOCKER=true vite build --mode docker && node scripts/generate-ssg.mjs",
		"build:critical": "tsc -b && vite build --mode critical && node scripts/generate-ssg.mjs",
		"build:e2e": "vite build --mode e2e",
		"build:images": "node scripts/process-images.mjs",
		"lint:fix": "eslint . --fix",
		"lint": "eslint .",
		"lint:css": "stylelint '**/*.scss'",
		"lint:css:fix": "stylelint '**/*.scss' --fix",
		"lint:mcp": "eslint --mcp",
		"sitemap": "node scripts/generate-sitemap.mjs",
		"preview": "vite preview",
		"serve:ssg": "node scripts/serve-ssg.mjs",
		"screenshot": "node scripts/screenshot.cjs",
		"screenshot:mobile-bg": "node scripts/screenshot-blurred-background.js",
		"test": "vitest run -c vitest.unit.config.ts --coverage",
		"test:storybook": "vitest run",
		"test:storybook:watch": "vitest",
		"test:storybook:a11y": "vitest run --reporter=verbose",
		"test:e2e": "npm run build:e2e && playwright test",
		"test:ci": "vitest run -c vitest.unit.config.ts --coverage",
		"typecheck": "tsc --noEmit",
		"verify:ssg": "node scripts/verify-ssg.mjs",
		"format:check": "prettier --check ./src",
		"format": "prettier --write ./src",
		"coverage:merge": "vitest coverage merge",
		"coverage:report": "vitest coverage report --reporter=lcov --reporter=text",
		"start": "node server/index.js",
		"heroku-postbuild": "vite build && node scripts/generate-ssg.mjs",
		"prepare": "husky",
		"storybook": "storybook dev -p 6006",
		"build-storybook": "STORYBOOK_BUILD=true storybook build",
		"release": "semantic-release",
		"lighthouse:ci": "lhci autorun",
		"perf:check": "node scripts/performance-check.mjs",
		"perf:report": "open PERFORMANCE_REPORT.html || xdg-open PERFORMANCE_REPORT.html"
	},
	"dependencies": {
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-icons": "^1.3.2",
		"@radix-ui/react-toast": "^1.2.15",
		"@radix-ui/react-toolbar": "^1.1.11",
		"@radix-ui/themes": "^3.2.1",
		"compression": "^1.8.1",
		"express": "^5.2.1",
		"express-static-gzip": "^3.0.0",
		"i18next": "^25.7.3",
		"i18next-browser-languagedetector": "^8.2.0",
		"i18next-fs-backend": "^2.6.1",
		"i18next-http-backend": "^3.0.2",
		"immer": "^11.0.1",
		"react": "^19.1.0",
		"react-dom": "^19.2.1",
		"react-ga4": "^2.1.0",
		"react-i18next": "^16.4.0",
		"react-is": "^19.2.1",
		"react-markdown": "^10.1.0",
		"react-router-dom": "^7.10.1",
		"recharts": "^3.6.0",
		"rehype-raw": "^7.0.0",
		"remark-gfm": "^4.0.1",
		"socket.io-client": "^4.8.1",
		"web-vitals": "^5.0.3",
		"zustand": "^5.0.9"
	},
