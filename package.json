		"@lhci/cli": "^0.15.1",
		"@playwright/test": "^1.57.0",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@semantic-release/changelog": "^6.0.3",
		"@semantic-release/git": "^10.0.1",
		"@semantic-release/github": "^12.0.2",
		"@storybook/addon-a11y": "^10.1.9",
